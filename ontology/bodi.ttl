#################################################################

###  http://w3id.org/bodi#LOCFormatCompliance
bodi:LOCFormatCompliance rdf:type owl:DatatypeProperty ;
                         rdfs:comment "Compliance of the file format to the Library of Congress Fle Formats Recommendations. May be \"preferred\", \"acceptable\" or \"not recommended\"." ;
                         rdfs:label "LOC Format Compliance" .


###  http://w3id.org/bodi#confidenceScore
bodi:confidenceScore rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain bodi:AIGeneratedText ;
                     rdfs:range xsd:decimal ;
                     rdfs:comment "A numerical value between 0 and 10 indicating the confidence level or reliability of AI-generated text. Higher values indicate greater confidence in the accuracy or quality of the generated content, supporting quality assessment and validation workflows." ;
                     rdfs:label "Confindence score" .


###  http://w3id.org/bodi#exceptionMessage
bodi:exceptionMessage rdf:type owl:DatatypeProperty ;
                      rdfs:domain bodi:Exception ;
                      rdfs:range rdfs:Literal ;
                      rdfs:comment "Contains the error message or description associated with an exception that occurred during processing. This property is essential for logging, debugging, and understanding failures in automated workflows." ;
                      rdfs:label "Exception message" .


###  http://w3id.org/bodi#hasDocumentation
bodi:hasDocumentation rdf:type owl:DatatypeProperty ;
                      rdfs:domain rico:Thing ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment "Provides a URI reference to official documentation, specifications, manuals, or other authoritative sources that describe the resource." ;
                      rdfs:label "Has documentation" .


###  http://w3id.org/bodi#hasHumanValidation
bodi:hasHumanValidation rdf:type owl:DatatypeProperty ;
                        rdfs:domain bodi:AIGeneratedText ;
                        rdfs:range rdfs:Literal ;
                        rdfs:comment "Indicates whether AI-generated text has been reviewed, verified, or validated by human experts. This property is crucial for quality assurance in systems that combine automated processing with human oversight." ;
                        rdfs:label "Has human validation" .


###  http://w3id.org/bodi#hierarchyDepth
bodi:hierarchyDepth rdf:type owl:DatatypeProperty ;
                    rdfs:domain rico:Instantiation ;
                    rdfs:range xsd:integer ;
                    rdfs:comment "Specifies the numerical depth level of a resource within a directory or organizational hierarchy, with level 0 representing the root." ;
                    rdfs:label "Hierarchy depth" .


###  http://w3id.org/bodi#omittedInformation
bodi:omittedInformation rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain rico:RecordResource ;
                        rdfs:comment "Indication of whether or not the resource has been anonymized." ;
                        rdfs:label "Omitted information" .


#################################################################
#    Classes
#################################################################

###  http://w3id.org/bodi#AIGeneratedText
bodi:AIGeneratedText rdf:type owl:Class ;
                     rdfs:subClassOf rico:Concept ;
                     rdfs:comment "Text generated through the use of artificial intelligence (AI)." ;
                     rdfs:label "AI generated text" .


###  http://w3id.org/bodi#Algorithm
bodi:Algorithm rdf:type owl:Class ;
               rdfs:subClassOf rico:Mechanism ;
               rdfs:comment "A computational procedure, formula, or set of rules designed to perform a specific task or solve a particular problem." ;
               rdfs:label "Algorithm" .


###  http://w3id.org/bodi#Exception
bodi:Exception rdf:type owl:Class ;
               rdfs:subClassOf rico:Concept ;
               rdfs:comment "An error condition, unexpected event, or abnormal situation that occurs during the execution of digital preservation processes. Exceptions are important for system monitoring, quality control, and troubleshooting automated workflows." ;
               rdfs:label "Exception" .


###  http://w3id.org/bodi#Software
bodi:Software rdf:type owl:Class ;
              rdfs:subClassOf rico:Mechanism ;
              rdfs:comment "A computer program, application, or digital tool that performs specific functions. Software entities are characterized by their functionality, version information, and technical specifications." ;
              rdfs:label "Software" .


###  http://w3id.org/bodi#SoftwareComponent
bodi:SoftwareComponent rdf:type owl:Class ;
                       rdfs:subClassOf rico:Mechanism ;
                       rdfs:comment "A modular, reusable part of a software system that provides specific functionality and can be combined with other components to create larger applications." ;
                       rdfs:label "Software component" .


###  http://w3id.org/bodi#SoftwareStack
bodi:SoftwareStack rdf:type owl:Class ;
                   rdfs:subClassOf rico:Agent ;
                   owl:disjointWith rico:Mechanism ;
                   rdfs:comment "A collection of mechanisms that performs an Activity together to provide a solution for a specific task." ;
                   rdfs:label "Software Stack" .


###  http://w3id.org/bodi#TechnicalMetadata
bodi:TechnicalMetadata rdf:type owl:Class ;
                       rdfs:subClassOf rico:Concept ;
                       rdfs:comment "The actual value or content of technical metadata associated with a digital resource. For example, in 'cp:revision = 1', the value '1' represents the TechnicalMetadata instance, while 'cp:revision' is the Technical Metadata Type. These values describe specific technical characteristics such as format parameters, creation settings, and other measurable attributes." ;
                       rdfs:label "Technical metadata" .


###  http://w3id.org/bodi#TechnicalMetadataType
bodi:TechnicalMetadataType rdf:type owl:Class ;
                           rdfs:subClassOf rico:Type ;
                           rdfs:comment "The type, property, or category that defines what kind of technical metadata is being described. For example, in 'cp:revision = 1', 'cp:revision' represents the TechnicalMetadataType that categorizes the metadata as revision information. This classification enables systematic organization of different kind of metadata like version identifiers, format specifications, encoding parameters, and preservation characteristics." ;
                           rdfs:label "Technical Metadata Type" .


###  http://w3id.org/bodi#TechnicalMetadataTypeSet
bodi:TechnicalMetadataTypeSet rdf:type owl:Class ;
                              rdfs:subClassOf rico:Concept ;
                              rdfs:comment "Group of Technical Metadata Types" ;
                              rdfs:label "Technical Metadata Type Set" .


