#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/bodi#generatedBy
bodi:generatedBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf rico:isRelatedTo ;
                 owl:inverseOf bodi:hasGenerated ;
                 rdfs:domain owl:Thing ;
                 rdfs:range rico:Agent ;
                 rdfs:comment "Indicates that a resource was created, produced, or brought into existence by a specific Agent or Activity. This property establishes a creation relationship between any resource and the agent pr activity responsible for its generation, whether through automated processes, human intervention, or hybrid approaches." ;
                 rdfs:label "Generated by" .


###  http://w3id.org/bodi#hasGenerated
bodi:hasGenerated rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isRelatedTo ;
                  rdfs:domain rico:Agent ;
                  rdfs:range owl:Thing ;
                  rdfs:comment "Indicates that an Agent or Activity has created, produced, or brought into existence a specific resource. This property represents the inverse relationship of 'generatedBy', allowing navigation from agents and activities to their generated outputs." ;
                  rdfs:label "Has generated" .


###  http://w3id.org/bodi#hasHashCode
bodi:hasHashCode rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf rico:isRelatedTo ;
                 owl:inverseOf bodi:isHashCodeOf ;
                 rdfs:domain rico:Instantiation ;
                 rdfs:comment "Associates an instantiation with its cryptographic hash code or checksum value. This property is crucial for digital preservation workflows, enabling integrity verification, duplicate detection, and authenticity validation of digital resources over time." ;
                 rdfs:label "Has hash code" .


###  http://w3id.org/bodi#hasOrHadSupervisor
bodi:hasOrHadSupervisor rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf rico:isRelatedTo ;
                        owl:inverseOf bodi:isOrWasSupervisorOf ;
                        rdfs:range rico:Agent ;
                        rdfs:comment "Indicates that an activity is or was supervised, managed, or overseen by a specific Agent. This property captures both current and historical supervisory relationships, essential for tracking accountability and governance in digital preservation workflows." ;
                        rdfs:label "Has or had supervisor" .


###  http://w3id.org/bodi#hasPreservationAssessment
bodi:hasPreservationAssessment rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf rico:isRelatedTo ;
                               owl:inverseOf bodi:isPreservationAssessmentOf ;
                               rdfs:domain rico:Instantiation ;
                               rdfs:range bodi:AIGeneratedText ,
                                          rico:RecordResource ;
                               rdfs:comment "Links an instantiation to its preservation assessment documentation, which may be either AI-generated text or traditional record resources." ;
                               rdfs:label "Has preservation assessment" .


###  http://w3id.org/bodi#hasRevisioned
bodi:hasRevisioned rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rico:isRelatedTo ;
                   owl:inverseOf bodi:revisedBy ;
                   rdfs:range bodi:AIGeneratedText ;
                   rdfs:comment "Connects a Person to the AI Generated Text that she has revised." ;
                   rdfs:label "Has revisioned" .


###  http://w3id.org/bodi#hasSameHashCodeAs
bodi:hasSameHashCodeAs rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rico:isInstantiationAssociatedWithInstantiation ;
                       rdf:type owl:SymmetricProperty ;
                       rdfs:range rico:Instantiation ;
                       rdfs:comment "Establishes that two digital instantiations have identical hash codes, indicating they are likely exact digital copies or duplicates." ;
                       rdfs:label "Has same hash code as" .


###  http://w3id.org/bodi#hasSoftwareComponent
bodi:hasSoftwareComponent rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf rico:hasOrHadPart ;
                          owl:inverseOf bodi:isSoftwareComponentOf ;
                          rdfs:domain bodi:SoftwareStack ;
                          rdfs:range rico:Mechanism ;
                          rdfs:comment "Indicates that a Software contains or includes a specific Software Component as one of its constituent parts. This property helps model the hierarchical structure of complex software environments." ;
                          rdfs:label "Has Software Component" .


###  http://w3id.org/bodi#hasStackElement
bodi:hasStackElement rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf rico:hasOrHadPart ;
                     owl:inverseOf bodi:isStackElementOf ;
                     rdfs:domain bodi:SoftwareStack ;
                     rdfs:range rico:Mechanism ;
                     rdfs:comment "Indicates that a software stack includes a specific Mechanism as one of its constituent elements. This property allows for modeling the various components (Software, Algorithms, Software Components) that together form a complete technological stack responsible for some Activity." ;
                     rdfs:label "Has stack element" .


###  http://w3id.org/bodi#hasTechnicalDescription
bodi:hasTechnicalDescription rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf rico:isRelatedTo ;
                             owl:inverseOf bodi:isTechnicalDescriptionOf ;
                             rdfs:domain rico:Instantiation ;
                             rdfs:range bodi:AIGeneratedText ,
                                        rico:RecordResource ;
                             rdfs:comment "Has technical description" ,
                                          "Links an instantiation to its technical description, which provides detailed information about the technical characteristics, structure, format, or implementation details of the resource." .


###  http://w3id.org/bodi#hasTechnicalMetadata
bodi:hasTechnicalMetadata rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf rico:isRelatedTo ;
                          owl:inverseOf bodi:isTechnicalMetadataOf ;
                          rdfs:domain rico:Instantiation ;
                          rdfs:range bodi:TechnicalMetadata ;
                          rdfs:comment "Associates an Instantiation with technical metadata that describes its technical properties, format specifications, creation parameters, or other characteristics." ;
                          rdfs:label "Has Technical Metadata" .


###  http://w3id.org/bodi#hasTechnicalMetadataType
bodi:hasTechnicalMetadataType rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf rico:hasOrHadType ;
                              owl:inverseOf bodi:isTechnicalMetadataTypeOf ;
                              rdfs:domain bodi:TechnicalMetadata ;
                              rdfs:range bodi:TechnicalMetadataType ;
                              rdfs:comment "Categorizes technical metadata according to the specific extracted field." ;
                              rdfs:label "Has technical metadata type" .


###  http://w3id.org/bodi#isHashCodeOf
bodi:isHashCodeOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isRelatedTo ;
                  rdfs:range rico:Instantiation .


###  http://w3id.org/bodi#isOrWasSupervisorOf
bodi:isOrWasSupervisorOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf rico:isRelatedTo ;
                         rdfs:domain rico:Agent ;
                         rdfs:label "Is or was supervisor of" .


###  http://w3id.org/bodi#isPreservationAssessmentOf
bodi:isPreservationAssessmentOf rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf rico:isRelatedTo ;
                                rdfs:domain bodi:AIGeneratedText ,
                                            rico:RecordResource ;
                                rdfs:range rico:Instantiation .


###  http://w3id.org/bodi#isSoftwareComponentOf
bodi:isSoftwareComponentOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf rico:isOrWasPartOf ;
                           rdfs:domain rico:Mechanism ;
                           rdfs:range bodi:SoftwareStack .


###  http://w3id.org/bodi#isStackElementOf
bodi:isStackElementOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf rico:isOrWasPartOf ;
                      rdfs:domain rico:Mechanism ;
                      rdfs:range bodi:SoftwareStack .


###  http://w3id.org/bodi#isTechnicalDescriptionOf
bodi:isTechnicalDescriptionOf rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf rico:isRelatedTo ;
                              rdfs:domain bodi:AIGeneratedText ,
                                          rico:RecordResource ;
                              rdfs:range rico:Instantiation .


###  http://w3id.org/bodi#isTechnicalMetadataOf
bodi:isTechnicalMetadataOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf rico:isRelatedTo ;
                           rdfs:domain bodi:TechnicalMetadata ;
                           rdfs:range rico:Instantiation .


###  http://w3id.org/bodi#isTechnicalMetadataTypeOf
bodi:isTechnicalMetadataTypeOf rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf rico:isOrWasTypeOf ;
                               rdfs:domain bodi:TechnicalMetadataType ;
                               rdfs:range bodi:TechnicalMetadata .


###  http://w3id.org/bodi#revisedBy
bodi:revisedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rico:isRelatedTo ;
               rdfs:domain bodi:AIGeneratedText ;
               rdfs:comment "Connects an AI Generated Text to the Person that revisioned the information." ;
               rdfs:label "Revised by" .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadPart
rico:hasOrHadPart rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isRelatedTo .


###  https://www.ica.org/standards/RiC/ontology#hasOrHadType
rico:hasOrHadType rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf rico:isRelatedTo .


###  https://www.ica.org/standards/RiC/ontology#isInstantiationAssociatedWithInstantiation
rico:isInstantiationAssociatedWithInstantiation rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf rico:isRelatedTo .


###  https://www.ica.org/standards/RiC/ontology#isOrWasPartOf
rico:isOrWasPartOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rico:isRelatedTo .


###  https://www.ica.org/standards/RiC/ontology#isOrWasTypeOf
rico:isOrWasTypeOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rico:isRelatedTo .


###  https://www.ica.org/standards/RiC/ontology#isRelatedTo
rico:isRelatedTo rdf:type owl:ObjectProperty .


#################################################################
