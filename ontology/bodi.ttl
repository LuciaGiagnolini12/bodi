<http://w3id.org/bodi#> rdf:type owl:Ontology ;
                         terms:contributor "Francesca Tomasi" ,
                                           "Paolo Bonora" ;
                         terms:creator "Lucia Giagnolini" ;
                         terms:modified "An extension of RiC-O (Records in Contexts Ontology) designed to represent the complexity of born-digital archival materials, addressing requirements for technical metadata, digital provenance, integrity verification, and contextual relationships."@en ;
                         rdfs:label "BoDi (Born-Digital) Ontology" ;
                         owl:imports "<http://iflastandards.info/ns/lrm/lrmoo/>"^^xsd:anyURI ,
                                     "<http://www.loc.gov/premis/rdf/v3/>"^^xsd:anyURI ,
                                     "<http://www.w3.org/ns/prov#>"^^xsd:anyURI ,
